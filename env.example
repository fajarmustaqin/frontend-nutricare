# Environment Variables untuk Production
# Copy file ini ke .env.local untuk development
# Atau set di Vercel Dashboard untuk production

# API Backend URL
REACT_APP_API_URL=https://your-backend-api.vercel.app

# Google OAuth (jika menggunakan)
REACT_APP_GOOGLE_CLIENT_ID=your_google_client_id

# MongoDB Connection (untuk backend)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/nutricare

# JWT Secret (untuk backend)
JWT_SECRET=your_jwt_secret_key

# Admin Credentials (untuk backend)
ADMIN_EMAIL=admin@nutricare.com
ADMIN_PASSWORD=admin123
